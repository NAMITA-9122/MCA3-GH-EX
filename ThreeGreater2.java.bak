class ThreeGreater2
{ 
	public static void main(String[] args)
	{
		int a=90,b=80,c=100;
		if(a>b)
		{
			if(a>c)
			{
				System.out.println("A is Greater "+c);
			}
		}
			else if(b>c)
				{
					System.out.println("B is Greater "+b);
				}
			else
				{
					System.out.println("C is Greater "+c);
				}
			
		
	}
}